ALGORITHM find the sum 
// the symmetric difference of two sets A and B
VAR
 so = setOps,
    Set1,Set2: ARRAY_OF INTEGER;
    k : INTEGER;
    
BEGIN
 Set 1 = [3, 1, 7, 9], 
 Set 2 = [2, 4, 1, 9, 3]
 sum=0 ;
 SET3 = so.difference(Set1,Set2);
FOR k FROM 0 TO SET3.length-1 STEP 1  DO
    sum = sum+SET3[k]
END_FOR
    write("the sum of all distinct elements from the set" sum);
    write(SET3)
END